{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "no-shadow": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "consistent-return": "off"
    }
}
